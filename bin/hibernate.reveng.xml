<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-catalog="PopcornFlavor" match-name="Actor" />
	<table-filter match-catalog="PopcornFlavor" match-name="Comment" />
	<table-filter match-catalog="PopcornFlavor" match-name="Genre" />
	<table-filter match-catalog="PopcornFlavor" match-name="Movie" />
	<table-filter match-catalog="PopcornFlavor" match-name="MovieActor" />
	<table-filter match-catalog="PopcornFlavor" match-name="MovieGenre" />
	<table-filter match-catalog="PopcornFlavor" match-name="User" />
</hibernate-reverse-engineering>